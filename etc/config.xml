<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <shqupsmychoice>
            <env>
                <extension_version>20.0.1</extension_version>
                <api_url>http://postapi.shipperhq.com/v1/upsMyChoice</api_url>
                <api_timeout>30</api_timeout>
                <js_bundle_url>https://assets.shipperhq.com/shq-upsmychoice_1.0.0.js</js_bundle_url>
            </env>
        </shqupsmychoice>
        <dev>
            <js>
                <minify_exclude>
                    <umc>https://assets.shipperhq.com</umc>
                </minify_exclude>
            </js>
        </dev>
    </default>
</config>